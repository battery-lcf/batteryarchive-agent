## Grab the directory of the running script
BIN_DIR=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )

set -a
source ${BIN_DIR}/../env
source ${BIN_DIR}/../key
set +a 

cd ${BIN_DIR}/../queries
python3 query_import.py --api_key ${ADMIN_API_KEY} --redash-url http://app:5000

cd ${BIN_DIR}
echo "Imported Queries"